{
  "name": "@primitivefi/rmm-ethers",
  "version": "0.0.5-beta.1",
  "description": "Primitive RMM Protocol ethers-based implementation.",
  "repository": {
    "type": "git",
    "url": "https://github.com/primitivefinance/rmm-ethers.git"
  },
  "homepage": "https://primitive.finance",
  "keywords": [
    "Primitive",
    "rmm",
    "Ethereum",
    "hardhat"
  ],
  "main": "dist/index.js",
  "typings": "./dist/index.d.ts",
  "module": "dist/rmm-ethers.esm.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn prepare",
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "deploy": "npx hardhat deploy",
    "prepare": "run-s prepare:*",
    "prepare:com": "hardhat compile",
    "prepare:typ": "hardhat typechain",
    "prepare:tsc": "tsc --project tsconfig.production.json",
    "prepare:api": "api-extractor run --local",
    "prepare:set-version": "node scripts/set-version.js",
    "docs": "run-s docs:*",
    "docs:collect-apis": "node scripts/collect-api.js",
    "docs:api-documenter": "api-documenter generate -i temp/sdk-api -o docs/sdk"
  },
  "author": "Primitive",
  "license": "MIT",
  "devDependencies": {
    "@microsoft/api-documenter": "^7.13.78",
    "@microsoft/api-extractor": "^7.19.3",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.3",
    "@primitivefi/hardhat-dodoc": "^0.1.3",
    "@primitivefi/rmm-sdk": "^1.0.1-beta.5",
    "@typechain/ethers-v5": "^8.0.5",
    "@typechain/hardhat": "^3.1.0",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.12.2",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "ethers": "^5.3.1",
    "hardhat": "^2.3.3",
    "hardhat-gas-reporter": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.0.0",
    "typechain": "^6.1.0",
    "typescript": "^4.3.2",
    "web3-units": "^1.3.3",
    "yarn-upgrade-all": "^0.5.4"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^3.3.0",
    "defender-relay-client": "^1.12.1"
  },
  "postinstall": "hardhat typechain",
  "release": {
    "branches": [
      "main"
    ]
  }
}
